.block#text
  .content
    %h2.title Message Queues
    .inner
      %table.table
        %tr
          %th.first Name
          %th Size
          %th Visibility
          %th.last &nbsp;
        - @queues.each do |q|
          %tr{:class => cycle("odd", "even")}
            %td= q.name
            %td= q.size
            %td= q.visibility
